<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
      <section name="DatabaseFactoryConfiguration" type="Liquinator.DAO.DatabaseFactorySectionHandler, Liquinator"/>
    </configSections>
    <connectionStrings>
        <add name="sql"
            connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\RecipeDatabase.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <DatabaseFactoryConfiguration Name="Liquinator.DAO.SqlDatabase" ConnectionStringName="sql" />

  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>
</configuration>